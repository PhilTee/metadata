<p id="notice"><%= notice %></p>

<h1>Listing Metakeys</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @metakeys.each do |metakey| %>
      <tr>
        <th><%= metakey.name %></th>
        <td><%= link_to 'Show', metakey %></td>
        <td><%= link_to 'Edit', edit_metakey_path(metakey) %></td>
        <td><%= link_to 'Destroy', metakey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Metakey', new_metakey_path %>
